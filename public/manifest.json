{
  "manifest_version": 3,
  "name": "cavo",
  "version": "1.0",
  "description": "cavo",
  "permissions": ["tabs", "storage","scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}